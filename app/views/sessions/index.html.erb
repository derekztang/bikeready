<h1>Welcome to BikeReady</h1>
<%= button_to "Sign Up", new_user_path %>

<%= form_for :session, url: login_path do |f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.email_field :email %>
  </p>

  <p>
    <%= f.label :password_digest %><br>
    <%= f.password_field :password_digest %>
  </p>

  <p>
    <%= f.submit 'log-in' %>
  </p>
<% end %>
